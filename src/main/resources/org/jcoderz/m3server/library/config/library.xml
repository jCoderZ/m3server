
<providers>
	<provider>
		<category>AUDIO</category>
		<name>m3library</name>
		<clazz>org.jcoderz.m3server.library.filesystem.FilesystemProvider
		</clazz>
		<parameters>
			<parameter>
				<key>root</key>
				<value>${M3_LIBRARY_HOME}/audio</value>
			</parameter>
			<parameter>
				<key>filters</key>
				<value>*.mp3</value>
			</parameter>
		</parameters>
	</provider>
	<provider>
		<category>VIDEO</category>
		<name>private</name>
		<clazz>org.jcoderz.m3server.library.filesystem.FilesystemProvider
		</clazz>
		<parameters>
			<parameter>
				<key>root</key>
				<value>${M3_LIBRARY_HOME}/video</value>
			</parameter>
			<parameter>
				<key>filters</key>
				<value>*.mpg|*.mpeg|*.mp4|*.3gp</value>
			</parameter>
		</parameters>
	</provider>
	<provider>
		<category>PHOTOS</category>
		<name>private</name>
		<clazz>org.jcoderz.m3server.library.filesystem.FilesystemProvider
		</clazz>
		<parameters>
			<parameter>
				<key>root</key>
				<value>${M3_LIBRARY_HOME}/photos</value>
			</parameter>
			<parameter>
				<key>filters</key>
				<value>*.jpg</value>
			</parameter>
		</parameters>
	</provider>
</providers>
